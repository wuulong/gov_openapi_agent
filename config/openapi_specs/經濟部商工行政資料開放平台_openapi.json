            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "Category": {
                    "type": "string"
                  },
                  "Category_Name": {
                    "type": "string"
                  },
                  "Classes": {
                    "type": "string"
                  },
                  "Classes_Name": {
                    "type": "string"
                  },
                  "Subcategory": {
                    "type": "string"
                  },
                  "Subcategories_Name": {
                    "type": "string"
                  },
                  "Business_Item": {
                    "type": "string"
                  },
                  "Business_Item_Desc": {
                    "type": "string"
                  },
                  "Business_Item_Content": {
                    "type": "string"
                  },
                  "Dgbas": {
                    "type": "string"
                  }
                }
              },
              "example": [
                {
                  "Category": "I",
                  "Category_Name": "專業、科學及技術服務業",
                  "Classes": "I1",
                  "Classes_Name": "顧問服務業",
                  "Subcategory": "I101",
                  "Subcategories_Name": "技術顧問業",
                  "Business_Item": "I101070",
                  "Business_Item_Desc": "農、林、漁、畜牧顧問業",
                  "Business_Item_Content": "從事農藝、園藝、林業及林業工程、漁撈、水產養殖及畜牧等技術事項之規劃、設計、監造（含施工簽證）、研究、分析、試驗、評價、鑑定、檢驗、計畫管理及其相關技術性服務。",
                  "Dgbas": "7609\t未分類其他專業、科學及技術服務業"
                }
              ]
            }
          }
        },
        "deprecated": false
      }
