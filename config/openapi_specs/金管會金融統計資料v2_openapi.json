  "paths": {
    "/API/CSV_EXPORT": {
      "get": {
        "tags": [
          "CSV_EXPORT"
        ],
        "summary": "取得資料表",
        "parameters": [
          {
            "name": "DATA_TYPE",
            "in": "query",
            "description": "1=月,2=年,空白=全部",
            "schema": {
              "type": "string",
              "default": ""
            }
          },
          {
            "name": "TableID",
            "in": "query",
            "description": "資料表代號,EX A01=公開發行公司股票發行概況統計表(1),詳細資料內容會公布在網頁中",
            "schema": {
              "type": "string",
              "default": "A01"
            }
          },
          {
            "name": "OUTPUT_FILE",
            "in": "query",
            "description": "Y=輸出檔案,N=網頁呈現",
            "schema": {
              "type": "string",
              "default": "N"
            }
          },
          {
            "name": "YY01",
            "in": "query",
            "description": "開始年月:yyyy/MM",
            "schema": {
              "type": "string",
              "default": "1912"
            }
          },
          {
            "name": "YY02",
            "in": "query",
            "description": "結束年月:yyyy/MM",
            "schema": {
              "type": "string",
              "default": "2999"
            }
          },
          {
            "name": "FFields",
            "in": "query",
            "description": "取得資料表",
            "schema": {
              "type": "string",
              "default": ""
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
