  "paths": {
    "/Activity.svc/GetActivityList": {
      "get": {
        "tags": [
          "Activity.svc"
        ],
        "summary": "取得體育活動清單",
        "description": "取得平台刊登之體育活動清單",
        "operationId": "activityList",
        "parameters": [
          {
            "name": "county",
            "in": "query",
            "required": false,
            "description": "縣市:\n  * `A` - 臺北市\n  * `B` - 臺中市\n  * `C` - 基隆市\n  * `D` - 臺南市\n  * `E` - 高雄市\n  * `F` - 新北市\n  * `G` - 宜蘭縣\n  * `H` - 桃園市\n  * `I` - 嘉義市\n  * `J` - 新竹縣\n  * `K` - 苗栗縣\n  * `M` - 南投縣\n  * `N` - 彰化縣\n  * `O` - 新竹市\n  * `P` - 雲林縣\n  * `Q` - 嘉義縣\n  * `T` - 屏東縣\n  * `U` - 花蓮縣\n  * `V` - 臺東縣\n  * `W` - 金門縣\n  * `X` - 澎湖縣\n  * `Z` - 連江縣\n",
            "schema": {
              "type": "string",
              "enum": [
                "A",
                "B",
                "C",
                "D",
                "E",
                "F",
                "G",
                "H",
                "I",
                "J",
                "K",
                "M",
                "N",
                "O",
                "P",
                "Q",
                "T",
                "U",
                "V",
                "W",
                "X",
                "Z"
              ]
            }
          },
          {
            "name": "activityKind",
            "in": "query",
            "required": false,
            "description": "活動類型:\n  * `1` - 體育賽事\n  * `2` - 系列活動\n  * `3` - 運動課程\n",
            "schema": {
              "type": "string",
              "enum": [
                "1",
                "2",
                "3"
              ]
            }
          },
          {
            "name": "activityDateBegin",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "活動日期查詢起日(民國年)，預設當月起日 EX:1080101\n"
          },
          {
            "name": "activityDateEnd",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "活動日期查詢訖日(民國年)，預設當月訖日 EX:1080131\n"
          },
          {
            "name": "paging",
            "in": "query",
            "required": false,
            "description": "是否需要分頁",
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "description": "分頁筆數",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "pageNo",
            "in": "query",
            "required": false,
            "description": "分頁頁碼",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ActivityList"
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ActivityList"
                  }
                }
              }
            }
          }
        }
      }
    },
