    "/SportsSituation.svc/GetSportsSituation": {
      "get": {
        "tags": [
          "SportsSituation.svc"
        ],
        "summary": "取得運動統計現況",
        "description": "取得運動統計現況",
        "operationId": "sportsSituation",
        "parameters": [
          {
            "name": "analyzeYear",
            "in": "query",
            "required": false,
            "description": "分析年度(民國年)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "analyzeTarget",
            "in": "query",
            "required": true,
            "description": "分析標的:\n  * `01` - 有無運動比例\n  * `02` - 不同程度運動比例\n  * `03` - 規律運動比例\n  * `04` - 運動目的比例\n  * `05` - 每週運動次數\n  * `06` - 運動時間\n  * `07` - 運動強度\n  * `08` - 縣市政府、學校、社區舉辦運動活動頻率\n  * `09` - 接收縣市政府宣傳運動訊息頻率\n  * `10` - 運動訊息來源\n  * `11` - 公眾運動場所設施滿意度\n  * `12` - 住家附近運動場所充足度\n  * `13` - 民眾喜好運動項目\n",
            "schema": {
              "type": "string",
              "enum": [
                "01",
                "02",
                "03",
                "04",
                "05",
                "06",
                "07",
                "08",
                "09",
                "10",
                "11",
                "12",
                "13"
              ]
            }
          },
          {
            "name": "statisticsItem",
            "in": "query",
            "required": false,
            "description": "統計項目:\n  * `age` - 年齡\n  * `city` - 縣市\n  * `sex` - 性別\n",
            "schema": {
              "type": "string",
              "enum": [
                "age",
                "city",
                "sex"
              ]
            }
          },
          {
            "name": "sort",
            "in": "query",
            "required": false,
            "description": "排序欄位:\n  * `statisticsItem` - 統計項目(例:有運動、沒有運動)\n  * `analyzeItem` - 分析項目 (例:男、女)\n",
            "schema": {
              "type": "string",
              "default": "statisticsItem",
              "enum": [
                "statisticsItem",
                "analyzeItem"
              ]
            }
          },
          {
            "name": "paging",
            "in": "query",
            "required": false,
            "description": "是否需要分頁",
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "description": "分頁筆數",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "pageNo",
            "in": "query",
            "required": false,
            "description": "分頁頁碼",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SportsSituation"
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SportsSituation"
                  }
                }
              }
            }
          }
        }
      }
    },
